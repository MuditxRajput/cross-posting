{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2017",
    "declaration": true,
    "esModuleInterop": true,
    "composite": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": [
    "*.ts",
    "models/*.ts" // Remove the trailing comma
  ],
  "exclude": ["node_modules", "dist"]
}